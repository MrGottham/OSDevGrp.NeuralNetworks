<?xml version="1.0" encoding="iso-8859-1"?>
<EstimateNet>
  <InputCategories>
    <InputCategory>
      <Name>Area</Name>
      <Description>The areal of the house</Description>
      <InputValue>
        <Name>&lt;= 50 m</Name>
        <Description>The areal is lower or equal to 50 m</Description>
      </InputValue>
      <InputValue>
        <Name>51 - 90 m</Name>
        <Description>The areal is between 51 and 90 m</Description>
      </InputValue>
      <InputValue>
        <Name>91 - 110 m</Name>
        <Description>The areal is between 91 and 110 m</Description>
      </InputValue>
      <InputValue>
        <Name>111 - 130 m</Name>
        <Description>The areal is between 111 and 130 m</Description>
      </InputValue>
      <InputValue>
        <Name>131 - 150 m</Name>
        <Description>The areal is between 131 and 150 m</Description>
      </InputValue>
      <InputValue>
        <Name>151 - 170 m</Name>
        <Description>The areal is between 151 and 170 m</Description>
      </InputValue>
      <InputValue>
        <Name>171 - 190 m</Name>
